{
	"env": {
		"browser": true,
		"node": true
	},
	"globals": {
		"chrome": true
	},
	"rules": {
		"no-console": 0,
		"no-empty": [1, { "allowEmptyCatch": true }],
		"import/prefer-default-export": "off"
	},
	"extends": ["next/core-web-vitals"],
  "overrides": [
    {
      "files": ["*.js"],
      "parser": "espree",
      "parserOptions": {
        "ecmaVersion": 2020
      }
    }
  ]
}
